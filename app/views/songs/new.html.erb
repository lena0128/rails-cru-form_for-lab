<h1>New Song Form</h1>

<%= form_for(@song) do |f| %>
<label for="name">Song Name: </label>
<%= f.text_field :name %><br><br>

<label for="artist_id">Artist ID: </label>
<%= f.text_field :artist_id %><br><br>

<label for="genre_id">Gnere ID: </label>
<%= f.text_field :genre_id %><br><br>

<%= f.submit %>
<% end %>    